{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "vehicle-subgraph": {
      "type": "application",
      "root": "apps/vehicle-subgraph",
      "entryFile": "main",
      "sourceRoot": "apps/vehicle-subgraph/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vehicle-subgraph/tsconfig.app.json"
      }
    },
    "account-subgraph": {
      "type": "application",
      "root": "apps/account-subgraph",
      "entryFile": "main",
      "sourceRoot": "apps/account-subgraph/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account-subgraph/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "redis-pubsub": {
      "type": "library",
      "root": "libs/redis-pubsub",
      "entryFile": "index",
      "sourceRoot": "libs/redis-pubsub/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-pubsub/tsconfig.lib.json"
      }
    },
    "redis-cache-gql": {
      "type": "library",
      "root": "libs/redis-cache-gql",
      "entryFile": "index",
      "sourceRoot": "libs/redis-cache-gql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-cache-gql/tsconfig.lib.json"
      }
    }
  }
}
